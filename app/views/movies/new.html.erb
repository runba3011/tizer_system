動画投稿

<%= form_with model: @movie , url: movies_path(@movie) , local: true do |f| %>
  <%= render partial: "shared/error_messages" , locals: {model: @movie} %>
  <%= f.label :title ,  "タイトル" %>
  <%= f.text_field :title , autofocus: true %>

  <%= f.label :link ,  "リンク" %>
  <%= f.text_field :link %>

  <%= f.label :single_point ,  "ポイント" %>
  <%= f.text_field :single_point %>

  <%= f.submit "作成" %>

  
<% end %>